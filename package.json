{
  "name": "@gofer-engine/source",
  "version": "0.0.0",
  "license": "ISC",
  "scripts": {
    "test-engine": "npx nx run engine:test",
    "test-handelse": "npx nx run handelse:test",
    "test-queue": "npx nx run queue:test",
    "test-stores": "npx nx run stores:test",
    "test-hl7": "npx nx run hl7:test",
    "test": "npx nx run-many -t test",
    "build": "npx nx run gofer-engine:build",
    "dev": "npx nx run gofer-engine:serve",
    "prepare": "husky install",
    "gen-pem": "openssl genrsa -out ./certs/key.pem",
    "gen-csr": "openssl req -new -key ./certs/key.pem -out ./certs/csr.pem -subj \"/C=US/ST=STATE/L=CITY/O=GOFER ENGINE/OU=ENGINE/CN=GOFER_ENGINE\"",
    "gen-cert": "openssl x509 -req -days 365 -in ./certs/csr.pem -signkey ./certs/key.pem -out ./certs/cert.pem",
    "generate-cert": "npm run gen-pem && npm run gen-csr && npm run gen-cert",
    "version-major": "npx nx release v major -p=stores,queue,handelse,engine,hl7",
    "version-minor": "npx nx release v minor -p=stores,queue,handelse,engine,hl7",
    "version-patch": "npx nx release v patch -p=stores,queue,handelse,engine,hl7",
    "publish": "nx release p -p=stores,queue,handelse,engine,hl7"
  },
  "private": true,
  "dependencies": {
    "axios": "^1.5.1",
    "dgraph-js": "^21.3.1",
    "graphql": "^16.8.1",
    "lodash": "^4.17.21",
    "mongodb": "^6.1.0",
    "postgresql-client": "^2.9.1",
    "surrealdb.js": "^0.9.1",
    "tslib": "^2.6.2",
    "unique-names-generator": "^4.7.1",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@jscutlery/semver": "^4.1.0",
    "@nx/esbuild": "16.10.0",
    "@nx/eslint-plugin": "16.10.0",
    "@nx/jest": "16.10.0",
    "@nx/js": "16.10.0",
    "@nx/linter": "16.10.0",
    "@nx/node": "16.10.0",
    "@nx/workspace": "16.10.0",
    "@swc-node/register": "^1.6.8",
    "@swc/core": "^1.3.92",
    "@types/jest": "^29.5.5",
    "@types/node": "~20.11.16",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "esbuild": "^0.19.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.10.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "nx": "16.10.0",
    "portfinder": "^1.0.32",
    "prettier": "^3.0.3",
    "semantic-release-npm-github-publish": "^1.5.4",
    "semantic-release-plus": "^20.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "10.9.1",
    "type-fest": "^4.6.0",
    "typescript": "^5.2.2",
    "verdaccio": "^5.26.3"
  }
}
